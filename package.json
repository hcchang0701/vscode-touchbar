{
	"name": "vscode-touchbar",
	"publisher": "hcchang0701",
	"displayName": "vscode_touchbar",
	"description": "vscode touchbar extension to facilitate development",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-touchbar.def",
		"onCommand:vscode-touchbar.ref",
		"onCommand:vscode-touchbar.fold",
		"onCommand:vscode-touchbar.unfold",
		"onCommand:vscode-touchbar.format"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-touchbar.def",
				"title": "def"
			},
			{
				"command": "vscode-touchbar.ref",
				"title": "ref"
			},
			{
				"command": "vscode-touchbar.fold",
				"title": "fold"
			},
			{
				"command": "vscode-touchbar.unfold",
				"title": "unfold"
			},
			{
				"command": "vscode-touchbar.format",
				"title": "fmt"
			}
		],
		"menus": {
			"touchBar": [
				{
					"command": "vscode-touchbar.def",
					"group": "debug"
				},
				{
					"command": "vscode-touchbar.ref",
					"group": "debug"
				},
				{
					"command": "vscode-touchbar.fold",
					"group": "view"
				},
				{
					"command": "vscode-touchbar.unfold",
					"group": "view"
				},
				{
					"command": "vscode-touchbar.format",
					"group": "editing"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
